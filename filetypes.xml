<application>
  <component name="CommunityFileTypes" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;.classpath;.project;" />
    <extensionMap>
      <mapping ext="mst" type="PLAIN_TEXT" />
      <mapping ext="hql" type="SQL files (syntax highlighting only)" />
      <mapping pattern="*.scala.html" type="Twirl files" />
      <mapping ext="jspx" type="XML" />
      <mapping ext="tagx" type="XML" />
    </extensionMap>
    <setting name="fileTypeChangedCounter" value="20" />
  </component>
  <component name="FileTypeManager" version="16">
    <extensionMap>
      <mapping ext="md" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>